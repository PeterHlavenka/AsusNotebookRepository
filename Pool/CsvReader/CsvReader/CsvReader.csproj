<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>WinExe</OutputType>
        <TargetFramework>net6.0-windows</TargetFramework>
        <Nullable>enable</Nullable>
        <UseWPF>true</UseWPF>
    </PropertyGroup>

    <ItemGroup>
      <None Remove="neco.csv" />
      <Content Include="neco.csv">
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </Content>
      <None Remove="Icons\Plus.png" />
      <Content Include="Icons\Plus.png">
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </Content>
      <None Remove="Icons\Ok.png" />
      <Content Include="Icons\Ok.png">
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </Content>
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="CsvHelper" Version="30.0.0" />
      <PackageReference Include="CsvReader.dll" Version="1.0.0" />
      <PackageReference Include="Syncfusion.Grid.WPF" Version="20.3.0.52" />
      <PackageReference Include="Syncfusion.GridExcelExport.Wpf" Version="20.3.0.52" />
    </ItemGroup>

</Project>
